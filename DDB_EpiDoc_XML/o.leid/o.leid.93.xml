    <lb n="3"/><expan>ἔσχ<ex>ομεν</ex></expan> <expan>ὑπ<ex>ὲρ</ex></expan> <expan>χω<ex>ματικοῦ</ex></expan> <expan>Ἀγο<ex>ρῶν</ex></expan> <expan>βο<ex>ρρᾶ</ex></expan> <num value="16">ιϛ</num> <expan><ex>ἔτους</ex></expan> <expan><unclear>ῥ</unclear>υ<ex>παρὰς</ex></expan> <expan><ex>δραχμὰς</ex></expan> ἑπτὰ <num value="4"><expan><ex>τετρώβολον</ex></expan></num>, 

